<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="nodetitle" title="Parent iD : {title}" contentWidth="auto" contentHeight="auto" resizable="true" draggable="true" showHeader="true" stretch="false" class="sapUiResponsivePadding--header">

           <content >
             <VBox width="500px">
                <HBox >
                    <!-- Add input fields for Service Profile Name, Rank, Description -->
                    <Label text="Service Parameter" textAlign="Begin"/>
                    <Input value="{/serviceName}" />
                </HBox>
                <HBox>
                <Label text="Rank" />
               
                <Input value="{/rank}" />
                </HBox>
            </VBox>
           </content>
            
            <beginButton>
                <Button text="Submit" press="onSubmit" />
            </beginButton>
             <endButton>
                <Button text="Cancel" press="onCancel" />
            </endButton>
           
    </Dialog>
</core:FragmentDefinition>
